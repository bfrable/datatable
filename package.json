{
  "name": "ucrsdtnet-datatable",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib:prod": "ng build ucrsdtnet-datatable --prod",
    "copy-files": "cp README.md dist/ucrsdtnet-datatable/README.md && cpx \"src/assets/**/*\" \"dist/ucrsdtnet-datatable/assets\" && cpx \"projects/ucrsdtnet-datatable/src/lib/themes/**/*\" \"dist/ucrsdtnet-datatable/themes\"",
    "build:css": "scss-bundle  -c ./scss-bundle.config.json && node-sass -o dist/ucrsdtnet-datatable/themes dist/ucrsdtnet-datatable/themes && node-sass -o dist/ucrsdtnet-datatable dist/ucrsdtnet-datatable",
    "test": "run-p lint test:unit",
    "test:unit": "ng test ucrsdtnet-datatable --watch=false",
    "test:watch": "ng test ucrsdtnet-datatable",
    "test:ci": "ng test ucrsdtnet-datatable --watch=false --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "run-s build:lib:prod copy-files build:css",
    "pack-table": "npm pack ./dist/ucrsdtnet-datatable/",
    "publish": "run-s publish:lib",
    "prepublishOnly": "echo \"This is the workspace -- you probably meant run 'npm run publish'.\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "husky": "^4.2.4",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^5.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.1.1",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
